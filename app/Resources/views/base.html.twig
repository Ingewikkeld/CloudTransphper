{% extends "MopaBootstrapBundle::base.html.twig" %}

{% block title %}CloudTransphper - Transphper your file with cloudVPS{% endblock %}

{% block head_bottom %}
<link href="/css/bootstrap.min.css" type="text/css" rel="stylesheet" media="screen" />

    <style type="text/css">
        body {
            padding-top: 20px;
            padding-bottom: 40px;
        }

            /* Custom container */
        .container-narrow {
            margin: 0 auto;
            max-width: 700px;
        }
        .container-narrow > hr {
            margin: 30px 0;
        }

            /* Main marketing message and sign up button */
        .jumbotron {
            margin: 60px 0;
            text-align: center;
        }
        .jumbotron h1 {
            font-size: 72px;
            line-height: 1;
        }
        .jumbotron .btn {
            font-size: 21px;
            padding: 14px 24px;
        }

            /* Supporting marketing content */
        .marketing {
            margin: 60px 0;
        }
        .marketing p + h4 {
            margin-top: 28px;
        }
    </style>
    <link href="/css/style.css" type="text/css" rel="stylesheet" media="screen" />
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap.file-input.js"></script>
{% endblock %}